interface info.grisge.zatel

type BondIface (
    slaves: []string,
    options: [string]string
)

type Iface (
    name: string,
    iface_type: string,
    state: (UP, DOWN, UNKNOWN),
    mtu: int,
    ?bond: BondIface
)

type NetState (
    ifaces: [string]Iface
)

method Get() -> (net_state: NetState)

error InternalError(msg: string)
